                            <br>
                            Email: <a href="mailto:contact@mii.co.id?subject=Contact From Website MII">contact@mii.co.id</a>
                        </p>
                    </div>
                    <!-- Grid column -->
                    <!-- Grid column -->
                    <div class="col-md-2 col-lg-2 col-xl-2 mx-auto mb-4">
                        <!-- Links -->
                        <h6 class="text-uppercase fw-bold mb-4">
                            Produk Kami
                        </h6>
                        <p>
                            <a href="#!" class="text-reset">Solution</a>
                        </p>
                        <p>
                            <a href="#!" class="text-reset">Service</a>
                        </p>
                        <p>
                            <a href="#!" class="text-reset">Use Case</a>
                        </p>
                        <p>
                            <a href="#!" class="text-reset">Bootcamp</a>
                        </p>
                    </div>
                    <!-- Grid column -->
                    <!-- Grid column -->
                    <div class="col-md-3 col-lg-2 col-xl-2 mx-auto mb-4">
                        <!-- Links -->
                        <h6 class="text-uppercase fw-bold mb-4">
                            Information For
                        </h6>
                        <p>
                            <a href="#!" class="text-reset">Bussines Partner</a>
                        </p>
                        <p>
                            <a href="#!" class="text-reset">Training</a>
                        </p>
                        <p>
                            <a href="#!" class="text-reset">Events</a>
                        </p>
                        <p>
                            <a href="#!" class="text-reset">Help</a>
                        </p>
                    </div>
                    <!-- Grid column -->
                    <!-- Grid column -->
                    <div class="col-md-4 col-lg-3 col-xl-3 mx-auto mb-md-0 mb-4">
                        <!-- Links -->
                        <h6 class="text-uppercase fw-bold mb-4">
                            Connect With Us
                        </h6>
                        <p><i class="fas fa-home me-3"></i> Jakarta, JKT 10000, ID</p>
                        <p>
                            <i class="fas fa-envelope me-3"></i>
                            dio@email.com
                        </p>
                        <p><i class="fas fa-phone me-3"></i> + 62 234 5678 9101</p>
                        <p><i class="fas fa-print me-3"></i> + 62 234 5678 9102</p>
                    </div>
                    <!-- Grid column -->
                </div>
                <!-- Grid row -->
            </div>
        </section>
        <!-- Section: Links  -->
        <!-- Copyright -->
        <div class="text-center p-4" style="background-color: rgba(0, 0, 0, 0.05);">
            © 2021 Copyright:
            <a class="text-reset fw-bold" href="https://mdbootstrap.com/">Dio Tri Eka Putra. All Rights Reserved.</a>
        </div>
        <!-- Copyright -->
    </footer>
